@model IEnumerable<ApplicationPerformance.Models.Objective>

@{
    ViewBag.Title = "Index";
}

<link href="@Url.Content("~/Content/themes/base/minified/jquery-ui.min.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.20.min.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {

        $("#ADDObjective").live("click", function (e) {
            var url = $(this).attr('href');
            $("#dialog-edit").dialog({
                title: 'Add New Objective',
                autoOpen: false,
                resizable: false,
                height: 455,
                width: 550,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: true,
                open: function (event, ui) {
                    $(this).load(url);

                },
                close: function (event, ui) {
                    $(this).dialog('close');
                }
            });

            $("#dialog-edit").dialog('open');
            return false;
        });
<h2>Index</h2>
        <b>
    <a id="ADDObjective" href="..\SystemUsers\Create">Add New Objective</a></b>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ObjectiveDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ObjectiveType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Confidentiality)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ObjectiveDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ObjectiveType).ToString().Replace("_", " ")
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Confidentiality)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ObjectiveID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ObjectiveID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ObjectiveID })
        </td>
    </tr>
}

</table>
